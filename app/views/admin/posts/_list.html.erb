<% posts.each do |post| %>
      <tr>
        <td>
          <%= link_to admin_post_path(post.id) do %>
          <%= image_tag post.image, size: "300x200" %>
          <% end %>
        </td>
      </tr>
      <td>
        <td>
          <%= link_to admin_user_path(post.user.id) do %>
          <%= image_tag post.user.get_profile_image(70,70) %>
          <% end %>
        </td>
      </td>
      <tr>
        <td>
          <%= post.introduction %>
        </td>
      </tr>
      <tr>
        <td>
          <%= post.user.name %>
        </td>
      </tr>
      <tr>
        <td>
          <%= post.created_at.strftime('%Y/%m/%d') %>
        </td>
      </tr>
    <% end %>